<h1>Categorias:</h1>
{{#each categoria}}
    <hr>
    <div class="card">
        <div class="card-box m-4">
            <h2>Nome: {{nome}}</h2>
            <h4>Slug: {{slug}}</h4>
            <a href="/categoria/{{slug}}"><button class="btn btn-info">Posts com essa categoria</button></a>            
        </div>
    </div>


{{else}}

{{/each}}